{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard",
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "./src/config/*.json"
    ]
  },
  "rules": {
    "import-name": false,
    "max-line-length": [
      true,
      80
    ]
  }
}
