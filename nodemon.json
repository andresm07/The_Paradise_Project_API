{
  "events": {
    "restart": "osascript -e 'display notification \"Reloaded\" with title \"Express TypeScript\"'",
    "crash": "osascript -e 'display notification \"I crashed...\" with title \"Express TypeScript\"'"
  },
  "ext": "*",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "src/**/*.spec.ts",
    "src/config/*.json"
  ],
  "verbose": true,
  "watch": [
    "src"
  ]
}
